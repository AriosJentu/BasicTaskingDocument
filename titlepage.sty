\RequirePackage{graphicx}
\newcommand{\drawer}{\vspace*{-10pt}}

\newcommand{\dhrule}[3] { % 1, 2 - lines height
	{
		\parindent=0pt
		\rule{\textwidth}{#1}
		
		\vspace{\dimexpr-\baselineskip+1mm+#3-1pt}
		
		\rule{\textwidth}{#2}
	}
}

\newcommand{\schoolfrom}[2] {
	
	\begin{center}
		\textbf{#1}	%School name
		
		\textbf{#2}	%Department name
	\end{center}
}

